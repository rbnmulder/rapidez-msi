{
    "name": "rapidez/msi",
    "description": "Rapidez MSI",
    "keywords": [
        "rapidez"
    ],
    "homepage": "https://rapidez.io",
    "license": "GPL-3.0",
    "authors": [
        {
            "name": "Robin Mulder",
            "email": "robin@justbetter.nl",
            "role": "Developer"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": "^7.4|^8.0",
        "illuminate/database": "^8.0",
        "illuminate/support": "^8.0",
        "illuminate/view": "^8.0",
        "rapidez/core": "~0.1"
    },
    "autoload": {
        "psr-4": {
            "Rapidez\\Msi\\": "src"
        }
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Rapidez\\Msi\\MsiServiceProvider"
            ]
        }
    }
}
